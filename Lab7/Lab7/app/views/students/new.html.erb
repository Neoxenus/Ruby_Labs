<h1>Registration</h1>
<%= form_for :student, url: students_path do |f| %>
  <p>
    Name<br>
    <%= f.text_field :name %>
  </p>
  <p>
    School<br>
    <%= f.text_field :school %>
  </p>
  <p>
    is_need_dorm<br>
    <%= f.check_box(:is_need_dorm, {}, "Yes", "No") %>
  </p>
  <p>
    is_teacher<br>
    <%= f.check_box(:is_teacher, {}, "Yes", "No") %>
  </p>
  <p>
    Experience<br>
    <%= f.number_field :experience, min: 0 %>
  </p>
  <p>
    language id<br>
    <%= f.number_field :language_id %>
  </p>
  <%= f.submit "Add new student" %>
<% end %>